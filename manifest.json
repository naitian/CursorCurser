{
    "manifest_version": 2,
    "name": "Cursor Curser",
    "version": "0.1.0",

    "description": "An obscene cursor",

    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "css": ["style.css"],
            "js": ["script.js", "curses.json"]
        },
        {
            "matches": ["https://*/*"],
            "css": ["style.css"],
            "js": ["script.js", "curses.json"]
        }
    ]
}
